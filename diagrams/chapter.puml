@startuml chapter
scale 3
skinparam dpi 300
skinparam classAttributeIconSize 0
skinparam backgroundColor transparent
skinparam classFontSize 14
skinparam classAttributeFontSize 13
skinparam padding 4
skinparam class {
  BackgroundColor white
  BorderColor #cccccc
  AttributeFontColor #000000
}
skinparam note {
  BackgroundColor #f5f5f5
  BorderColor #cccccc
  FontColor #333333
}


class "chapter" {
  multilingual-text title
  multilingual-text description
  URI url
  Duration time-required
  multilingual-text learning-goal
  learning-objectives-entry[] learning-objectives
  __
  <i><color:#666666>language language?</color></i>
  <i><color:#666666>supplemental-material[] supplemented-by?</color></i>
}

note bottom of "chapter"
  Beschreibung eines Kapitels.
  
  - title: verwendet für Zitationen
  - description: Inhaltsbeschreibung
  - url: URL zur Webansicht
  - time-required: geschätzte Verarbeitungszeit
  - learning-goal: Lernziel
  - learning-objectives: Lernziele
  - language (optional): Sprache
  - supplemented-by (optional): Zusatzmaterialien
end note

@enduml
