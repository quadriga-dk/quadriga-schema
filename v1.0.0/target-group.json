{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://quadriga-dk.github.io/quadriga-schema/v1.0.0/target-group.json",
  "title": "target-group – Zielgruppe",
  "description": "Zielgruppe\n\nenglish translation:\n\n- Forschende (PostDoc) → Researchers (PostDoc)\n- Forschende (Projektleitung) → Researchers (Project lead)\n- Promovierende → PhD students\n- Hochschullehrende → University lecturers",
  "type": "array",
  "minItems": 1,
  "items": {
    "enum": [
      "Forschende (PostDoc)",
      "Forschende (Projektleitung)",
      "Promovierende",
      "Hochschullehrende"
    ]
  },
  "x-mappings": {
    "dc": null,
    "dcterms": {
      "relation": "skos:closeMatch",
      "target": "dcterms:audience"
    },
    "lrmi": {
      "relation": "skos:closeMatch",
      "target": "lrmi:educationalAudience"
    },
    "modalia": {
      "relation": "skos:exactMatch",
      "target": "modalia:hasTargetGroup"
    },
    "schema": {
      "relation": "skos:closeMatch",
      "target": "schema:audience"
    }
  }
}
