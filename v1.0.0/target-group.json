{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://quadriga-dk.github.io/quadriga-schema/v1.0.0/target-group.json",
  "type": "array",
  "description": "Zielgruppe<br><br>english translation:<br>--------------------<br>Forschende (PostDoc) → Researchers (PostDoc)<br>Forschende (Projektleitung) → Researchers (Project lead)<br>Promovierende → PhD students<br>Hochschullehrende → University lecturers",
  "minItems": 1,
  "items": {
    "enum": [
      "Forschende (PostDoc)",
      "Forschende (Projektleitung)",
      "Promovierende",
      "Hochschullehrende"
    ]
  },
  "x-mappings": {
    "dc": null,
    "dcterms": {
      "relation": "skos:closeMatch",
      "property": "dcterms:audience"
    },
    "lrmi": {
      "relation": "skos:closeMatch",
      "property": "lrmi:educationalAudience"
    },
    "modalia": {
      "relation": "skos:exactMatch",
      "property": "modalia:TargetGroup"
    },
    "schema": {
      "relation": "skos:closeMatch",
      "property": "schema:audience"
    }
  }
}
