{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://quadriga-dk.github.io/quadriga-schema/v1.0.0/date-issued.json",
  "title": "date-issued – Veröffentlichungsdatum",
  "description": "Datum, an dem das Buch (erst-)veröffentlicht wurde.\n\nVeröffentlichungen von Vorabversionen (Alpha-, Beta-Versionen) gelten nicht als Veröffentlichung.",
  "type": "string",
  "format": "date",
  "x-mappings": {
    "dc": {
      "relation": "skos:broadMatch",
      "target": "dc:date"
    },
    "dcat": null,
    "dcterms": {
      "relation": "skos:exactMatch",
      "target": "dcterms:issued"
    },
    "lrmi": null,
    "modalia": {
      "relation": "skos:exactMatch",
      "target": "schema:datePublished"
    },
    "schema": {
      "relation": "skos:exactMatch",
      "target": "schema:datePublished"
    }
  }
}
