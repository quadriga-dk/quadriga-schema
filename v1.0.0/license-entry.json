{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://quadriga-dk.github.io/quadriga-schema/v1.0.0/license-entry.json",
  "oneOf": [
    {
      "type": "string",
      "format": "uri",
      "description": "URL zur Lizenz"
    },
    {
      "type": "object",
      "description": "Lizenzname und URL zur Lizenz",
      "required": ["name", "url"],
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "format": "uri"
        }
      }
    }
  ],
  "x-mappings": {
    "dc": {
      "relation": "skos:exactMatch",
      "property": "dc:rights"
    },
    "dcterms": {
      "relation": "skos:exactMatch",
      "property": "dcterms:license"
    },
    "lrmi": null,
    "modalia": null,
    "schema": {
      "relation": "skos:exactMatch",
      "property": "schema:license"
    }
  }
}
